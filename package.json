    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "http-proxy-middleware": "^2.0.3",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "lru-cache": "^7.4.0",
    "mobx": "^6.4.2",
    "mobx-react-lite": "^3.3.0",
    "mobx-react-router": "^4.1.0",
